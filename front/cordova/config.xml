        <splash density="port-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
        <preference name="Orientation" value="portrait" />
        <uses-permission name="android.permission.ACCESS_COARSE_LOCATION" platform="android" />
        <uses-permission name="android.permission.ACCESS_FINE_LOCATION" platform="android" />
        <uses-permission name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" platform="android" />
    </platform>
    <plugin name="cordova-plugin-background-mode" spec="^0.7.2" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.1" />
    <plugin name="org.apache.cordova.geolocation" spec="^0.3.6" />
    <plugin name="cordova.plugins.diagnostic" spec="^4.0.10" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="android" spec="^7.0.0" />
